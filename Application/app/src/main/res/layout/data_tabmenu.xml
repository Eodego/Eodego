<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center|top"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageButton
            android:id="@+id/datainfobtn"
            android:layout_width="0px"
            android:layout_height="130px"
            android:layout_weight="1"
            android:background="@drawable/info_press"
            android:onClick="dataClick"
            android:scaleType="centerCrop"
            android:text="정보" />

        <ImageButton
            android:id="@+id/datamenubtn"
            android:layout_width="0px"
            android:layout_height="130px"
            android:layout_weight="1"
            android:background="@drawable/menu_press"
            android:onClick="dataClick"
            android:scaleType="centerCrop"
            android:text="메뉴" />

        <ImageButton
            android:id="@+id/datareviewbtn"
            android:layout_width="0px"
            android:layout_height="130px"
            android:layout_weight="1"
            android:background="@drawable/review_press"
            android:onClick="dataClick"
            android:scaleType="centerCrop"
            android:text="리뷰" />
    </LinearLayout>

    <FrameLayout
        android:id="@+id/dataframe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    </FrameLayout>
</LinearLayout>